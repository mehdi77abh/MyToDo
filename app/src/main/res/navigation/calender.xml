<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/calender"
    app:startDestination="@id/calenderFragment">

    <fragment
        android:id="@+id/calenderFragment"
        android:name="com.example.mytodo.calendarFragment.CalendarFragment"
        android:label="fragment_calender"
        tools:layout="@layout/fragment_calender" >
        <action
            android:id="@+id/action_calenderFragment_to_addTaskFragment2"
            app:destination="@id/addTaskFragment2" />
        <action
            android:id="@+id/action_calenderFragment_to_editTaskFragment2"
            app:destination="@id/editTaskFragment2" />
    </fragment>
    <fragment
        android:id="@+id/addTaskFragment2"
        android:name="com.example.mytodo.taskListFragment.AddTaskFragment"
        android:label="AddTaskFragment" />
    <fragment
        android:id="@+id/editTaskFragment2"
        android:name="com.example.mytodo.taskListFragment.EditTaskFragment"
        android:label="EditTaskFragment" />
</navigation>